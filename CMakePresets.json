{
    "version": 9,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "Debug64",
            "displayName": "Debug x64",
            "binaryDir": "${sourceDir}/Temp/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/Install/${presetName}"
            }
        },
        {
            "name": "Release64",
            "displayName": "Release x64",
            "binaryDir": "${sourceDir}/Temp/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/Install/${presetName}"
            }
        },
        {
            "name": "Debug32",
            "displayName": "Debug x86",
            "binaryDir": "${sourceDir}/Temp/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/Install/${presetName}"
            },
            "architecture": {
                "value": "x86",
                "strategy": "external"
            }
        },
        {
            "name": "Release32",
            "displayName": "Release x86",
            "binaryDir": "${sourceDir}/Temp/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/Install/${presetName}"
            },
            "architecture": {
                "value": "x86",
                "strategy": "external"
            }
        }
    ]
}
